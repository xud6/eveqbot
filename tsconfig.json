{
   "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": true,
      "target": "es2015",
      "esModuleInterop": true,
      "strictNullChecks": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictFunctionTypes": true,
      "strictBindCallApply": true,
      "strictPropertyInitialization": false,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "resolveJsonModule": true
   }
}